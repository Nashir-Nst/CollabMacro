<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cream"
    tools:context=".NonEmergencyActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="396dp"
        android:layout_height="73dp"
        android:background="@color/red"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="-1dp">

        <ImageView
            android:id="@+id/back"
            android:layout_marginLeft="20dp"
            android:layout_width="38.8dp"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:src="@drawable/back"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck"
            tools:layout_editor_absoluteY="8dp" />



        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="15dp"
            android:background="@drawable/searchbar"
            android:orientation="horizontal">

            <EditText
                android:id="@+id/searchBar"
                android:layout_width="225dp"
                android:layout_height="45dp"
                android:background="@drawable/searchbar"
                android:hint="asdasd"
                android:padding="20dp"
                android:textColorHint="@color/grey"
                tools:ignore="TouchTargetSizeCheck" />

            <ImageView
                android:layout_width="31dp"
                android:layout_height="35dp"
                android:paddingTop="10dp"
                android:src="@drawable/search"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.77"
                app:layout_constraintStart_toStartOf="@+id/toolbar"
                tools:layout_editor_absoluteY="16dp" />

        </LinearLayout>


        <ImageView
            android:id="@+id/grid"
            android:layout_width="30dp"
            android:layout_height="50dp"
            android:layout_marginEnd="30dp"
            android:src="@drawable/grid"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck"
            tools:layout_editor_absoluteY="4dp" />

    </androidx.appcompat.widget.Toolbar>



    <ImageView
        android:id="@+id/kecelakaan"
        android:layout_width="348dp"
        android:layout_height="104dp"
        android:layout_marginTop="80dp"
        android:src="@drawable/img_22"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <ImageView
        android:id="@+id/pendarahan"
        android:layout_width="348dp"
        android:layout_height="104dp"
        android:layout_marginTop="428dp"
        android:src="@drawable/img_26"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <ImageView
        android:id="@+id/kepala"
        android:layout_width="348dp"
        android:layout_height="104dp"
        android:layout_marginTop="348dp"
        android:src="@drawable/img_25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <ImageView
        android:id="@+id/asma"
        android:layout_width="348dp"
        android:layout_height="104dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/img_21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <ImageView
        android:id="@+id/luka"
        android:layout_width="348dp"
        android:layout_height="104dp"
        android:layout_marginTop="512dp"
        android:src="@drawable/img_27"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <ImageView
        android:id="@+id/jantung"
        android:layout_width="348dp"
        android:layout_height="104dp"
        android:layout_marginTop="168dp"
        android:src="@drawable/img_23"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <ImageView
        android:id="@+id/cpr"
        android:layout_width="348dp"
        android:layout_height="104dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/img_20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.577"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <ImageView
        android:id="@+id/tenggelam"
        android:layout_width="348dp"
        android:layout_height="104dp"
        android:layout_marginTop="260dp"
        android:src="@drawable/img_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

</androidx.constraintlayout.widget.ConstraintLayout>

